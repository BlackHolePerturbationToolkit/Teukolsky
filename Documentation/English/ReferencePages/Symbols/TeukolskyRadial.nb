 CellID->2077539814,ExpressionUUID->"baed8942-c882-4633-a11d-eecb2348d4f2"],

Cell[TextData[{
 "The solutions produced by TeukolskyRadial satisfy the homogeneous Teukolsky \
radial equation ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"[", 
           RowBox[{
             SuperscriptBox["\[CapitalDelta]", 
              RowBox[{"-", 
                StyleBox["s", "TI"]}]], 
             FractionBox[
              StyleBox["d", "TI"], 
              RowBox[{
                StyleBox["d", "TI"], 
                StyleBox["r", "TI"]}]], 
             RowBox[{"(", 
               RowBox[{
                 SuperscriptBox["\[CapitalDelta]", 
                  RowBox[{
                    StyleBox["s", "TI"], "+", "1"}]], 
                 FractionBox[
                  StyleBox["d", "TI"], 
                  RowBox[{
                    StyleBox["d", "TI"], 
                    StyleBox["r", "TI"]}]]}], ")"}], "+", 
             FractionBox[
              RowBox[{
                SuperscriptBox[
                 StyleBox["K", "TI"], "2"], "-", "2", 
                StyleBox["i", "TI"], 
                StyleBox["s", "TI"], 
                RowBox[{"(", 
                  RowBox[{
                    StyleBox["r", "TI"], "-", 
                    StyleBox["M", "TI"]}], ")"}], 
                StyleBox["K", "TI"]}], "\[CapitalDelta]"], "+", "4", 
             StyleBox["i", "TI"], 
             StyleBox["s", "TI"], "\[Omega]", 
             StyleBox["r", "TI"], "-", 
             SubscriptBox["\[Null]", 
              StyleBox["s", "TI"]], 
             SubscriptBox["\[Lambda]", 
              RowBox[{"\[ScriptL]", 
                StyleBox["m", "TI"]}]]}], "]"}], 
         SubscriptBox["\[Null]", 
          StyleBox["s", "TI"]], 
         SubscriptBox["\[Psi]", 
          RowBox[{"\[ScriptL]", 
            StyleBox["m", "TI"], "\[Omega]"}]], "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\bigg[\\Delta^{-s} \\frac{d}{dr} \\bigg( \\Delta^{s+1}\\frac{d \
}{dr}\\bigg)\n  +\\frac{K^2 - 2 i s (r-M)K}{\\Delta} + 4 i s \\omega r - {}_s \
\\lambda_{\\ell m} \\bigg]{}_{s} \\psi_{\\ell m \\omega} = 0", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],
  CellChangeTimes->{{3.8975309660006104`*^9, 3.897530973927291*^9}, 
    3.8975312195623093`*^9},ExpressionUUID->
  "c9fd54d8-dd73-40f7-99c3-508198c7fcfd"],
 " where ",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        StyleBox["K", "TI"], "\[LongEqual]", 
        RowBox[{"(", 
          RowBox[{
            SuperscriptBox[
             StyleBox["r", "TI"], "2"], "+", 
            SuperscriptBox[
             StyleBox["a", "TI"], "2"]}], ")"}], "\[Omega]", "-", 
        StyleBox["a", "TI"], 
        StyleBox["m", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
    "K=(r^2+a^2)\\omega - a m", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]],
  CellChangeTimes->{{3.8975314235017443`*^9, 3.897531426346553*^9}},
  ExpressionUUID->"268bf919-00f8-455e-afc7-745de3aca0f8"],
 " and ",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{"\[CapitalDelta]", "\[LongEqual]", 
        SuperscriptBox[
         StyleBox["r", "TI"], "2"], "-", "2", 
        StyleBox["M", "TI"], 
        StyleBox["r", "TI"], "+", 
        SuperscriptBox[
         StyleBox["a", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
    "input" -> "\\Delta = r^2 - 2 M r + a^2", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]],
  CellChangeTimes->{3.897531450996215*^9},ExpressionUUID->
  "8a45fad9-e161-4ee7-b41d-bbfee70475db"],
 ". "
}], "Notes",
 CellChangeTimes->{{3.897530370928481*^9, 3.8975303989698753`*^9}, {
   3.897530642631846*^9, 3.897530648449898*^9}, {3.897530815040489*^9, 
   3.897530859038587*^9}, 3.89753091195081*^9, 3.897530963151802*^9, {
   3.8975310964433203`*^9, 3.897531111999919*^9}, {3.897531229589902*^9, 
