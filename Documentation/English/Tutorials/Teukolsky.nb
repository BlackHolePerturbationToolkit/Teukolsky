8dae-4565db18c1e4"],

Cell[BoxData[
 RowBox[{"TeukolskyRadial", "[", 
  RowBox[{"s", ",", "l", ",", "m", ",", "a", ",", "\[Omega]", ",", 
   RowBox[{"Method", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<NumericalIntegration\>\"", ",", 
      RowBox[{"\"\<Domain\>\"", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\"\<In\>\"", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"3", ",", "20"}], "}"}]}], ",", 
         RowBox[{"\"\<Up\>\"", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"4", ",", "16"}], "}"}]}]}], "}"}]}]}], "}"}]}]}], 
  "]"}]], "Input",
