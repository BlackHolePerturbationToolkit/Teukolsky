}, Open  ]],

Cell[CellGroupData[{

Cell["Normalisation convention", \
"Subsubsection",ExpressionUUID->"d490037a-0154-4439-9f95-329a13e30e8c"],

Cell["\<\
The homogeneous solutions are normalised such that they have transmission \
coefficient equal to 1, i.e.\
\>", "Text",ExpressionUUID->"b69ce055-1c46-492d-a234-f8623e1b58f5"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Limit]", 
      RowBox[{"r", "\[Rule]", 
       SubscriptBox["r", "+"]}]], 
     RowBox[{"|", 
      RowBox[{
       SuperscriptBox["\[CapitalDelta]", "s"], 
       RowBox[{
        SuperscriptBox["R", "in"], "(", "r", ")"}]}], "|"}]}], "=", "1"}], 
   TraditionalForm]],ExpressionUUID->"ff450687-279e-49b0-b8f7-139e750c14a3"],
 ",\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox["\[Limit]", 
      RowBox[{"r", "\[Rule]", "\[Infinity]"}]], 
     RowBox[{"|", 
      RowBox[{
       SuperscriptBox["r", 
        RowBox[{
         RowBox[{"2", "s"}], "+", "1"}]], 
       RowBox[{
        SuperscriptBox["R", "up"], "(", "r", ")"}]}], "|"}]}], "=", "1"}], 
   TraditionalForm]],ExpressionUUID->"894a92ca-b584-4237-aaec-53b377bcf675"],
 "."
}], "Text",
 TextAlignment->-0.5,ExpressionUUID->"dda5c70f-4cda-4b6b-8919-053bc0f70302"],

Cell[TextData[{
 "We can verify this by plotting the \[OpenCurlyDoubleQuote]up\
\[CloseCurlyDoubleQuote] solution at large radius. It should have the \
asymptotic form ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[TildeEqual]", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[ImaginaryI]", " ", "\[Omega]", " ", 
      SubscriptBox["r", "*"]}]]}], TraditionalForm]],ExpressionUUID->
  "6b2116a6-d6cb-40d8-9803-25751bd4b7c1"],
 " where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["r", "*"], " ", "=", 
    RowBox[{"r", "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"2", 
        SubscriptBox["\[Kappa]", "+"]}]], "ln", " ", 
      FractionBox[
       RowBox[{"r", "-", 
        SubscriptBox["r", "+"]}], 
       RowBox[{"2", "M"}]]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"2", 
        SubscriptBox["\[Kappa]", "-"]}]], "ln", " ", 
      FractionBox[
       RowBox[{"r", "-", 
        SubscriptBox["r", "-"]}], 
       RowBox[{"2", "M"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "ae1b4ff1-b597-4f56-91e3-bb9c907fe42c"],
 " is the tortoise coordinate with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Kappa]", "\[PlusMinus]"], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["r", "\[PlusMinus]"], "-", 
      SubscriptBox["r", "\[MinusPlus]"]}], 
     RowBox[{"2", 
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["r", "\[PlusMinus]", "2"], "-", 
        SuperscriptBox["a", "2"]}], ")"}]}]]}], TraditionalForm]],
  ExpressionUUID->"35449820-fda9-4b51-9940-ded8f7029865"],
 " the surface gravity on the outer/inner horizon defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["r", "\[PlusMinus]"], "=", 
    RowBox[{"M", "\[PlusMinus]", 
     SqrtBox[
      RowBox[{
       SuperscriptBox["M", "2"], "-", 
       SuperscriptBox["a", "2"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "cce7cb2a-9ac5-48a8-94c2-ffd878c59dc1"],
 "."
}], "Text",ExpressionUUID->"ce7215c4-248a-4d7f-b6d4-476de8c8e7e8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Rdata", "=", 
   RowBox[{"Transpose", "[", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"ReIm", "[", 
       RowBox[{
        SuperscriptBox["r", 
         RowBox[{
          RowBox[{"2", "s"}], "+", "1"}]], 
        RowBox[{
         RowBox[{"R", "[", "\"\<Up\>\"", "]"}], "[", "r", "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"r", ",", "3000", ",", "3100", ",", "5"}], "}"}]}], "]"}], 
    "]"}]}], ";"}]], "Input",
 CellLabel->"In[16]:=",ExpressionUUID->"0ec28e87-0566-4e71-9ad6-e337f36bc207"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"rp", "=", 
   RowBox[{"1", "+", 
    SqrtBox[
     RowBox[{"1", "-", 
      SuperscriptBox["a", "2"]}]]}]}], ";", 
  RowBox[{"rm", "=", 
   RowBox[{"1", "-", 
    SqrtBox[
     RowBox[{"1", "-", 
      SuperscriptBox["a", "2"]}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rstar", "[", "r_", "]"}], ":=", 
   RowBox[{"r", "+", 
    RowBox[{
     FractionBox[
      RowBox[{"2", " ", "rp"}], 
      RowBox[{"rp", "-", "rm"}]], " ", 
     RowBox[{"Log", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"r", "-", "rp"}], ")"}], "/", "2"}], "]"}]}], "-", 
    RowBox[{
     FractionBox[
      RowBox[{"2", " ", "rm"}], 
      RowBox[{"rp", "-", "rm"}]], " ", 
     RowBox[{"Log", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"r", "-", "rm"}], ")"}], "/", "2"}], "]"}]}]}]}], 
  ";"}]}], "Input",
 CellLabel->"In[21]:=",ExpressionUUID->"9905b839-82da-48e8-9496-7afa9fb91d09"],

Cell[BoxData[
 RowBox[{"Show", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"ListPlot", "[", 
    RowBox[{"Rdata", ",", 
     RowBox[{"DataRange", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{"3000", ",", "3100"}], "}"}]}]}], "]"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"Evaluate", "[", 
      RowBox[{"ReIm", "[", 
       RowBox[{"Exp", "[", 
        RowBox[{"\[ImaginaryI]", " ", "\[Omega]", " ", 
         RowBox[{"rstar", "[", "r", "]"}]}], "]"}], "]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "3000", ",", "3100"}], "}"}]}], "]"}]}], 
  "]"}]], "Input",
 CellLabel->
  "In[24]:=",ExpressionUUID->"0fc09dfb-fb40-45b2-a281-7e198c315802"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Properties", \
"Subsubsection",ExpressionUUID->"ad9e4a53-97c4-42a8-aaec-458fa417310e"],

Cell[TextData[{
 "Finally, we can query a ",
 ButtonBox["TeukolskyRadialFunction",
  BaseStyle->"Link",
  ButtonData->"paclet:Teukolsky/ref/TeukolskyRadialFunction"],
 " about its properties. For example, we can obtain the values of the \
asymptotic amplitudes (as expected from our normalisation convention, the \
transmission coefficient is 1):"
}], "Text",ExpressionUUID->"ae05093a-398b-4e8d-8a81-a5916af1e816"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R", "[", "\"\<In\>\"", "]"}], "[", "\"\<Amplitudes\>\"", 
  "]"}]], "Input",
 CellLabel->"In[25]:=",ExpressionUUID->"6c2301e1-7890-4c76-9f69-cd8e13c1a29e"],

Cell[TextData[{
 "The full set of properties can be obtained using ",
 ButtonBox["Keys",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Keys"],
 ":"
}], "Text",ExpressionUUID->"1dcf4b1e-bd9d-49e3-8aa5-d3465cd5053b"],

Cell[BoxData[
 RowBox[{"keys", "=", 
  RowBox[{"Keys", "[", 
   RowBox[{"R", "[", "\"\<In\>\"", "]"}], "]"}]}]], "Input",
 CellLabel->"In[26]:=",ExpressionUUID->"927c4a52-dfce-4e41-9952-cd1eebf202eb"],

Cell[BoxData[
 RowBox[{
  RowBox[{"R", "[", "\"\<In\>\"", "]"}], "/@", 
  RowBox[{"{", 
   RowBox[{
   "\"\<s\>\"", ",", "\"\<l\>\"", ",", "\"\<m\>\"", ",", "\"\<a\>\"", ",", 
